<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.Erag0.ReversoContextBot.Entity.UsersEntity" table="Users" schema="main">
        <id name="id">
            <column name="id" sql-type="integer" not-null="true"/>
        </id>
        <property name="chatId">
            <column name="chat_id" sql-type="text" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="text" not-null="true"/>
        </property>
        <property name="command">
            <column name="command" sql-type="text" not-null="true"/>
        </property>
        <property name="language">
            <column name="language" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>